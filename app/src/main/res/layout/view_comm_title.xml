<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content">
    <com.flowerbell.gejigeji.widget.StrokeTextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/y70"
        android:fontFamily="@font/hylxjt"
        tools:text="个人信息"
        android:layout_centerHorizontal="true"
        android:textSize="@dimen/x32"
        app:StrokeTextViewHasBorder="true"
        app:StrokeTextViewInnerColor="@color/color_fdd329"
        app:StrokeTextViewOuterColor="@color/color_7b492a"
        app:StrokeTextViewStroke="8px" />
    <ImageView
        android:id="@+id/iv_return"
        android:paddingLeft="@dimen/x50"
        android:paddingTop="@dimen/y70"
        android:paddingRight="@dimen/x50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/return_ic"/>
</RelativeLayout>