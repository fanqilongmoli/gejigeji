<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flowerbell.gejigeji">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/AppTheme">
        <activity android:name=".modules.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.my.MyInfoActivity" />
        <activity android:name=".modules.my.MyWalletActivity" />
        <activity android:name=".modules.my.RechargeActivity" />
        <activity android:name=".modules.my.WithdrawActivity" />
        <activity android:name=".modules.my.AddressManageActivity" />
        <activity android:name=".modules.farm.MessageActivity"/>
    </application>

</manifest>